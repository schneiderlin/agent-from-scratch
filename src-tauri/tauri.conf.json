{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "agent-from-scratch",
  "version": "0.1.0",
  "identifier": "com.agent-from-scratch.app",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "agent-from-scratch",
        "url": "http://localhost:8080",
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
